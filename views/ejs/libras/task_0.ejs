        playerVars: {cc_lang_pref:'en', cc_load_policy: 0, autoplay:0 ,rel: 0, controls: 0, disablekb: 1},
        height: '360',
        width: '640',
  autohide: '1',
        events: {
          'onReady': onPlayerReady,
          'onStateChange': onPlayerStateChange
        }
      }
    );
  }
  function onPlayerReady(event) {
    duration = (input.end - input.start) + 0.5;
    player.loadVideoById(input.uri, parseFloat(input.start));
    setInterval(function() {syncTimeline();}, 100);
 }

 function syncTimeline(){
  var progress = 100 * (player.getCurrentTime() - input.start) / (input.end -input.start );
  timeline.style.width =  progress + '%';
   }
 
  function onPlayerStateChange(event) {
    if(player.getCurrentTime() > input.end || player.getCurrentTime() < input.start){
      player.seekTo(parseFloat(input.start));
      player.pauseVideo();
    $("#bt_play_pause").text('Play');  

    }else{
      if (event.data == YT.PlayerState.PLAYING) {
        setTimeout(restartVideoSection, duration * 1000);
      }
    }
  }
  function restartVideoSection() {
    if(player.getCurrentTime() > input.end){
      player.seekTo(parseFloat(input.start));
      player.pauseVideo();
    $("#bt_play_pause").text('Play');  
    }
  }

  function syncRange(){
    var index = list_marks.selectedIndex;
  var range = (list_marks.options[index].value).split(':');
  player.seekTo(range[0]);
  $( "#slider" ).slider( "values", [parseFloat(range[0])-0.1,parseFloat(range[1])] );
  }

  function delRange(){        
        var r = confirm('Are you sure that want remove this range?');
        if(r == true){
                var range = list_marks.selectedIndex;
                list_marks.selectedIndex = range - 1;
                list_marks.remove(range);
      }
  }

  function addRange(){
      var start = parseFloat($( "#slider" ).slider( "values", 0 ))+0.1 ;
      var stop =  parseFloat($( "#slider" ).slider( "values", 1 )) ;
            var Op = document.createElement("OPTION");
            Op.text = start + " - " + stop;
            Op.value = start+":"+stop;
            list_marks.options[list_marks.options.length] = Op;
            list_marks.selectedIndex = list_marks.options.length -1;
    
  }


