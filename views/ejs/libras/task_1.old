  function playPause(){
	if(player.getPlayerState() == 1){
		player.pauseVideo();
		bt_play_pause.text = 'Play';
	}else{
		player.playVideo();
		bt_play_pause.text = 'Pause';
	}
  }

  function tiltTime(delta){
    player.pauseVideo();
	bt_play_pause.text = 'Play';
    player.seekTo(player.getCurrentTime() + delta);
  }

